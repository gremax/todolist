%h2 Register

.col-md-8.col-md-offset-2
  %form{ng: {submit: "register()"}}
    .form-group
      %input.form-control{type: "email", placeholder: "Email", ng: {model: "user.email"}}
    .form-group
      %input.form-control{type: "password", placeholder: "Password", ng: {model: "user.password"}}
    %input.btn.btn-primary{ttype: "submit", value: "Register"}
